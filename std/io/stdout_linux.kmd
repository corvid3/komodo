# make sure to mark the struct as exportable and
struct Stdout [export, global]:
	filedes: usize

impl Write for Stdout:
	defn write(&self, in slice: []const u8) [export] -> usize:
		std.os.linux.syscalls.write(self.filedes, slice)

impl New for Stdout:
	defn new() -> Self:
		return Stdout {
			filedes: 0
		}
